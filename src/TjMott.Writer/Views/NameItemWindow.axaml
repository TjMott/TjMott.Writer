<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" CanResize="False"
        x:Class="TjMott.Writer.Views.NameItemWindow"
		WindowStartupLocation="CenterOwner"
        Title="Set Name" Height="110" Width="300">
	<Grid>
		<Grid>
			<Grid.RowDefinitions>
				<RowDefinition Height="Auto" />
				<RowDefinition Height="*" />
			</Grid.RowDefinitions>
			<TextBox Name="nameTextBox" Margin="5" KeyDown="nameTextBox_KeyDown" />
			<StackPanel Grid.Row="1" Classes="dialogButtonPanel">
				<Button Name="okButton" Click="okButton_Click">OK</Button>
				<Button Name="cancelButton" Click="cancelButton_Click">Cancel</Button>
			</StackPanel>
		</Grid>
	</Grid>
</Window>
